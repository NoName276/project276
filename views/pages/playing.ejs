<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
      
          
    <% function getHashParams() {   %>
    <%    var hashParams = {};      %>
    <%    var e, r = /([^&;=]+)=?([^&;]*)/g,  %>
    <%        q = window.location.hash.substring(1);   %>
    <%    while ( e = r.exec(q)) {  %>
    <%       hashParams[e[1]] = decodeURIComponent(e[2]); %>
    <%    } %>
    <%    return hashParams;    %>
    <% }    %>
    
    <% const params = getHashParams(); %>

  </body>
</html>